<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="FB_Argument" Id="{a8de1bab-544f-463d-b649-c52e572fc35a}" SpecialFunc="None">
    <Declaration><![CDATA[{attribute 'no_explicit_call' := 'do not call this POU directly'}
FUNCTION_BLOCK FB_Argument IMPLEMENTS I_Argument
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	sValue:					STRING(255); 
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
    </Implementation>
    <Method Name="M_AddBOOL" Id="{6fe5c2f5-9f6f-45ef-a512-7b981b0be320}">
      <Declaration><![CDATA[METHOD PUBLIC M_AddBOOL : I_Argument
VAR_INPUT
	bValue:				BOOL;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[sValue:= CONCAT(sValue, BOOL_TO_STRING(bValue));			// convert
sValue:= CONCAT(sValue, '$R');				// add separator
M_AddBOOL:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_AddINT" Id="{0fb61d34-2130-4e2b-8ca3-a66334bd442e}">
      <Declaration><![CDATA[METHOD PUBLIC M_AddINT : I_Argument
VAR_INPUT
	nValue:				LINT;
END_VAR
VAR
	sTemp: 			STRING;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[sValue:= CONCAT(sValue, CONCAT(LINT_TO_STRING(nValue), '$R'));	// add new arg
M_AddINT:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_AddREAL" Id="{ec2078de-d198-403e-9f1d-9aa3713c42cf}">
      <Declaration><![CDATA[METHOD PUBLIC M_AddREAL : I_Argument
VAR_INPUT
	fValue:				LREAL;
	nDecimals:			USINT;
END_VAR
VAR
	sTemp: 			STRING;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[sValue:= CONCAT(sValue, CONCAT(LREAL_TO_FMTSTR(fValue, nDecimals, TRUE), '$R'));				// add new arg
M_AddREAL:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_AddSTRING" Id="{6ddea08b-8528-414a-b8b0-ca7a04ccfa78}">
      <Declaration><![CDATA[METHOD PUBLIC M_AddSTRING : I_Argument
VAR_INPUT
	sValue:				STRING(255);
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[THIS^.sValue:= CONCAT(THIS^.sValue, CONCAT(sValue, '$R'));				// add separator 
M_AddSTRING:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_AddTIME" Id="{5bf7bbc0-6a06-4191-858d-e9e3e764bb18}">
      <Declaration><![CDATA[METHOD PUBLIC M_AddTIME : I_Argument
VAR_INPUT
	tValue:				TIME;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[sValue:= CONCAT(sValue, CONCAT(TIME_TO_STRING(tValue), '$R'));				// add separator
M_AddTIME:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_Clear" Id="{1a899f54-f4aa-43d6-ba56-2488949d893d}">
      <Declaration><![CDATA[METHOD PUBLIC M_Clear : I_Argument
VAR_INPUT
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[sValue:= '';
M_Clear:= THIS^;]]></ST>
      </Implementation>
    </Method>
    <Property Name="P_Value" Id="{364579c4-8829-400e-a5c9-b1603a0f22c8}">
      <Declaration><![CDATA[PROPERTY PUBLIC P_Value : string(255)]]></Declaration>
      <Get Name="Get" Id="{a740fb78-092e-4273-9e56-34b846282910}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[P_Value:= sValue;]]></ST>
        </Implementation>
      </Get>
    </Property>
  </POU>
</TcPlcObject>