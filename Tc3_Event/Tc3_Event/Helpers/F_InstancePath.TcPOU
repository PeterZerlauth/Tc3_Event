<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1">
  <POU Name="F_InstancePath" Id="{3e94a824-6472-4e8f-9269-d3c52fddc58e}" SpecialFunc="None">
    <Declaration><![CDATA[// Filter instance path, to something useful
FUNCTION F_InstancePath : STRING(255)
VAR_INPUT
	sInstancePath:			STRING(255);
END_VAR
VAR
    nPosition     : INT;
END_VAR

]]></Declaration>
    <Implementation>
      <ST><![CDATA[F_InstancePath:= RIGHT(sInstancePath, LEN(sInstancePath) - FIND(sInstancePath, '.'));
F_InstancePath:= RIGHT(F_InstancePath, LEN(F_InstancePath) - FIND(F_InstancePath, '.'));

FOR nPosition:= LEN(F_InstancePath) TO 1 BY -1 DO
    IF F_InstancePath[nPosition] = 46 THEN
		F_InstancePath := LEFT(F_InstancePath, nPosition);
		RETURN;
	END_IF
END_FOR]]></ST>
    </Implementation>
  </POU>
</TcPlcObject>